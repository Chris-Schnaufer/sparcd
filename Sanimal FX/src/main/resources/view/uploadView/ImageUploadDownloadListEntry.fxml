<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<HBox fx:id="mainPane" alignment="CENTER_LEFT" prefHeight="135.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controller.uploadView.ImageUploadDownloadListEntryController">
    <stylesheets>
        <URL value="@ImageUploadDownloadListEntry.css"/>
    </stylesheets>
    <children>
        <ImageView fitHeight="60" preserveRatio="true">
            <Image url="@../../images/uploadWindow/uploadedFile.png"/>
            <HBox.margin>
                <Insets left="5" right="5"/>
            </HBox.margin>
        </ImageView>
        <VBox alignment="CENTER_LEFT" HBox.hgrow="ALWAYS">
            <Label fx:id="lblUsername" text="Upload Username" VBox.vgrow="ALWAYS">
                <VBox.margin>
                    <Insets left="5" right="5" top="5"/>
                </VBox.margin>
                <font>
                    <Font size="20.0"/>
                </font>
            </Label>
            <Label fx:id="lblDate" text="Upload Date" VBox.vgrow="ALWAYS">
                <VBox.margin>
                    <Insets left="5" right="5" top="5"/>
                </VBox.margin>
                <font>
                    <Font size="20.0"/>
                </font>
            </Label>
            <Label fx:id="lblTagged" text="Tagged ???">
                <VBox.margin>
                    <Insets left="5" right="5" top="5"/>
                </VBox.margin>
                <font>
                    <Font size="20.0"/>
                </font>
            </Label>
            <Label fx:id="lblEdits" text="Edit">
                <VBox.margin>
                    <Insets left="5" right="5" top="5"/>
                </VBox.margin>
                <font>
                    <Font size="20.0"/>
                </font>
            </Label>
        </VBox>
        <VBox spacing="5">
            <HBox.margin>
                <Insets left="10" top="5" right="5"/>
            </HBox.margin>
            <Button fx:id="btnUpload" prefHeight="60" prefWidth="60" onAction="#uploadPressed">
                <graphic>
                    <ImageView fitHeight="40" fitWidth="40">
                        <Image url="@../../images/uploadWindow/uploadIconNoBackground.png"/>
                    </ImageView>
                </graphic>
            </Button>
            <Button fx:id="btnDownload" prefHeight="60" prefWidth="60" onAction="#downloadPressed">
                <graphic>
                    <ImageView fitHeight="40" fitWidth="40">
                        <Image url="@../../images/uploadWindow/downloadIconNoBackground.png"/>
                    </ImageView>
                </graphic>
            </Button>
        </VBox>
    </children>
</HBox>
