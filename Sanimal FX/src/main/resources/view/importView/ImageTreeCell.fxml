<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<?import javafx.scene.image.ImageView?>
<?import javafx.geometry.Insets?>
<HBox fx:id="mainPane" alignment="CENTER_LEFT" spacing="5" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="controller.ImageTreeCellController" prefHeight="40"
    onDragEntered="#cellDragEntered" onDragDropped="#cellDragDropped" onDragExited="#cellDragExited" onDragOver="#cellDragOver">
    <stylesheets>
        <URL value="@ImageTreeCell.css"/>
    </stylesheets>
    <children>
        <ImageView fx:id="imgIcon" fitHeight="40" fitWidth="40"/>
        <Label fx:id="lblText" HBox.hgrow="ALWAYS"/>
    </children>
</HBox>
